<!-- 电脑键盘遥控机器人节点-->

<launch>

  <node pkg="handsfree_teleop" type="handsfree_teleop_key.py" name="handsfree_teleop_keyboard" output="screen">
    <param name="scale_linear" value="0.3" type="double"/>
    <param name="scale_angular" value="0.5" type="double"/>
    <remap from="/cmd_vel" to="/mobile_base/mobile_base_controller/cmd_vel" />
  </node>
  
</launch>
